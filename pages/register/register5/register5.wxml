<!--pages/register/register5/register5.wxml-->
<view class="contain">
  <view class="title">你自认为是......</view>
  <view class="tip">男人，女人，还是牙星人？</view>
  <view wx:if="{{gender==0}}" class="check">
    <view class="option" bindtap="boy">男人</view>
    <view class="option2" bindtap="girl">女人</view>
    <view class="option2" bindtap="other">牙星人</view>
  </view>
  <view wx:if="{{gender==1}}" class="check">
    <view class="option2" bindtap="boy">男人</view>
    <view class="option" bindtap="girl">女人</view>
    <view class="option2" bindtap="other">牙星人</view>
  </view>
  <view wx:if="{{gender==2}}" class="check">
    <view class="option2" bindtap="boy">男人</view>
    <view class="option2" bindtap="girl">女人</view>
    <view class="option" bindtap="other">牙星人</view>
  </view>
  <image class="image" src="https://z3.ax1x.com/2021/07/21/WwBMtJ.png"></image>
  <view bindtap="toNext" class="next">
    <view class="jixu">继续</view>
    <image class="arrow" src="https://z3.ax1x.com/2021/07/21/WwB311.png"></image>
  </view>
</view>
